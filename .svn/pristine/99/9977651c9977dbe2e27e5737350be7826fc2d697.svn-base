#TJK_dropDownMenu li a:focus,
#TJK_dropDownMenu li a:active {color:#ffa902; background:url(../img/nav_active.jpg) no-repeat;}
/* position and z-index for the sub-menus */
#TJK_dropDownMenu li:hover ul,
#TJK_dropDownMenu li ul {left:0;z-index:10}
