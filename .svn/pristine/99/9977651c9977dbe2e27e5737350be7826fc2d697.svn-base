	position:relative !important; /* ie Mac */
	cursor:pointer !important;
	white-space:nowrap;
	padding:0 8px;
	line-height:20px;
	height:20px;
	font-weight:400;
	color:#666;
	background:none;
	font-size:12px;
}
#TJK_dropDownMenu li ul li a:hover,#TJK_dropDownMenu li ul li a:focus,#TJK_dropDownMenu li ul li a:active{
	background:#D5F5A7;
}

