	cursor:pointer;
	float:left;
	text-align:center;
	list-style-type:none;
	font-weight:bold;
	font-size:14px;

	
}
/* sub-menu LIs */
#TJK_dropDownMenu li ul li {
	
	border:none;
	width:auto;
}
/* All anchors */
#TJK_dropDownMenu li a {
	color:#666;
	text-decoration:none;
	display:block;
	height:28px;
	line-height:28px;;
	width:67px;
	padding:0 8px;
}

