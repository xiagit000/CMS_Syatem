<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <%@taglib uri="/struts-tags" prefix="s"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title><s:text name="fileUploadError"/></title>
</head>
<body>
<style type="text/css">
.error {
	margin-top: 50px;
	width: 800px;
	margin: 0 auto;
	margin-top: 50px;
	height: 120px;
	border: 3px #c2c2c2 dashed;
	background: #F8FED1;
	padding: 30px;
}

.error p {
	font-size: 2.4em;
	margin-top: 12px;
	color: #777;
}

h2 {
	font-size: 2.2em;
	font-weight: 600;
	color: #FF3300;
	border-bottom: 2px #c2c2c2 solid;
	padding: 3px;
}
</style>
	<div class="error">
    	<h2><s:text name="fileUploadError"/></h2>
        <p><s:fielderror key="attachment" theme="simple"></s:fielderror></p>
        <p><a href="javascript:history.go(-1);"><s:text name="clickToBack"/></a></p>
    </div>
</body>
</html>