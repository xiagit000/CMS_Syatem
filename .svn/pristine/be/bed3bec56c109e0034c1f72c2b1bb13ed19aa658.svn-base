#menu .nav {
  height: 41px;
  font-family: arial, verdana, sans-serif;
  font-size: 14px;
  width: 1000px;
  z-index: 100;
  position: relative;
  /* IE6 only */
}
#menu .nav .table {
  display: table;
  margin: 0 auto;
}
#menu .nav .select, #menu .nav .current {
  margin: 0;
  padding: 0;
  list-style: none;
  display: table-cell;
  white-space: nowrap;
}
#menu .nav li {
  margin: 0;
  padding: 0;
  height: auto;
  float: left;
}
#menu .nav .select a {
  display: block;
  height: 41px;
  width: 111px;
  font-weight: bold;
  background: url(../images/nav_commonbg.jpg);
  text-decoration: none;
  text-align: center;
  line-height: 41px;
  white-space: nowrap;
  color: #006795;
}
#menu .nav .select a:hover,
#menu .nav .select li:hover a {
  background: url(../images/nav_hoverbg.jpg);
  cursor: pointer;
  text-align: center;
  color: #fff;
}
#menu .nav .select a b {
  font-weight: bold;
  font-size: 14px;
}
#menu .nav .select_sub {
  display: none;
  position: absolute;
  width: auto;
  top: 41px;
  background-color: gray;
  padding: 0;
  z-index: 100;
  font-size: 12px;
  color: #fff;
}
#menu .nav .select_sub a {
  background: none;
  color: white;
}
#menu .nav .select_sub a:hover {
  background: url(../images/nav_sechoverbg.jpg) no-repeat 5px 4px;
}
#menu .nav table {
  border-collapse: collapse;
  margin: -1px;
  font-size: 1em;
  width: 0;
  height: 0;
}
#menu .nav .sub {
  display: table;
  margin: 0 auto;
  padding: 0;
  list-style: none;
}
#menu .nav .select :hover .select_sub,
#menu .nav .current .show {
  display: block;
  position: absolute;
  width: auto;
  top: 41px;
  background-color: gray;
  padding: 0;
  z-index: 100;
  font-size: 12px;
  color: #fff;
}
#menu .nav .current .show {
  z-index: 10;
}
#menu .nav .select :hover .sub li a,
#menu .nav .current .show .sub li a {
  background: transparent;
  white-space: nowrap;
  color: #fff;
}
#menu .nav .current .sub li.sub_show a {
  color: #fff;
  cursor: default;
}
#menu .nav .select .sub li a {
  font-weight: normal;
  height: 32px;
  line-height: 32px;
}
#menu .nav .select :hover .sub li a:hover,
#menu .nav .current .sub li a:hover {
  background: none;
  visibility: visible;
  background: url(../images/nav_sechoverbg.png) no-repeat 6px 6px;
  color: #fff;
}

